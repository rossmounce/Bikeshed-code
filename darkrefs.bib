@Article{Ford2008,
  Title                    = {{It's All Academic: Google Scholar, Scirus, and Windows Live Academic Search}},
  Author                   = {Ford, Lyle and O'Hara, Lisa H.},
  Journal                  = {Journal of Library Administration},
  Year                     = {2008},

  Month                    = mar,
  Number                   = {3-4},
  Pages                    = {43--52},
  Volume                   = {46},

  Abstract                 = {{SUMMARY In April 2006 and again in November 2006, Google Scholar, Scirus, and Windows Live Academic Search were searched for a set of 39 citations found in databases covering science, computer science, electrical engineering, and physics, along with the British Library OPAC and WorldCAT. The functionality and coverage of each engine are compared. Changes in results and functionality between April and November 2006 are also noted. Google Scholar retrieved the most citations, followed by Scirus, and then by Microsoft's engine. Libraries should link to these engines and enable OpenURL and other link resolving systems to ease retrieval for their users.}},
